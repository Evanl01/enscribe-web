{
  "name": "enscribe-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "build:prod": "NODE_ENV=production next build",
    "deploy:s3": "npm run build:prod && aws s3 sync out/ s3://enscribe-web-prod-static --delete --cache-control 'public, max-age=31536000, immutable' --exclude '*.html' && aws s3 sync out/ s3://enscribe-web-prod-static --delete --cache-control 'public, max-age=0, must-revalidate' --exclude '*' --include '*.html'",
    "deploy:cloudfront": "aws cloudfront create-invalidation --distribution-id ERTMP9FOP3O1Q --paths '/*'",
    "deploy:full": "npm run deploy:s3 && npm run deploy:cloudfront"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.4",
    "marked": "^17.0.1",
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "styled-components": "^6.1.19",
    "supabase": "^2.65.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "babel-plugin-react-compiler": "1.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "tailwindcss": "^4"
  }
}
